<project name="CompileProject" default="compile" basedir=".">
    <target name="compile">
    	<echo message="Ant project:CompileProject target:compile class.path:${class.path}"/>
    	<javac	srcdir="${java.src}"
				destdir="${java.cls}"
				deprecation="off"
				classpath="${class.path}"
    			includeantruntime="true"
    	/>
    </target>
</project>
